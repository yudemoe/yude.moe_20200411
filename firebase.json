{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [ {
      "source" : "**/*.@(jpg|jpeg|gif|png|webp)",
    "headers" : [ {
      "key" : "Cache-Control",
      "value" : "max-age=5184000"
    } ]
  }
]
  }
}
